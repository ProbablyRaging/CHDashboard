<%- include('partials/header') -%>
    <%- include('partials/topbar') -%>
        <%- include('partials/container') -%>

            <div class="content-page-title">
                <p>Welcome, <%= username %>
                </p>
            </div>
            <p class="content-page-description">Choose a category below to view its contents or edit its settings</p>

            <!-- Content Box -->
            <div class="content-box">
                <div class="content-box-div">

                    <!-- Content Box Inner -->
                    <header class="content-box-title-class">
                        <div class="content-box-title-flex">
                            <h5 class="content-box-title-decor">
                                <div class="content-box-title">Command Management</div>

                            </h5>
                        </div>
                    </header>

                    <hr class="content-box-separator">

                    <div>
                        <div class="content-box-content">
                            <!-- Dashboard Button -->
                            <div class="content-box-button" onclick="location.href = '/settings/index';">
                                <div class="button-img-container">
                                    <img src="https://cdn-icons-png.flaticon.com/512/281/281760.png" class="button-img">
                                </div>
                                <h5 class="button-title">Index</h5>
                                <div class="button-description">Change the responses for the index command</div>
                            </div>

                            <!-- Dashboard Button -->
                            <div class="content-box-button" onclick="location.href = '/settings/rules';">
                                <div class="button-img-container">
                                    <img src="https://cdn-icons-png.flaticon.com/512/281/281760.png" class="button-img">
                                </div>
                                <h5 class="button-title">Rule</h5>
                                <div class="button-description">Change the rules everwhere they are used on the
                                    server</div>
                            </div>
                        </div>
                    </div>
                    <!--  -->
                </div>
            </div>
            <!--  -->

            <!-- Content Box -->
            <div class="content-box">
                <div class="content-box-div">

                    <!-- Content Box Inner -->
                    <header class="content-box-title-class">
                        <div class="content-box-title-flex">
                            <h5 class="content-box-title-decor">
                                <div class="content-box-title">Server Logs</div>

                            </h5>
                        </div>
                    </header>

                    <hr class="content-box-separator">

                    <div>
                        <div class="content-box-content">
                            <!-- Dashboard Button -->
                            <div class="content-box-button" onclick="location.href = '/logs/warnings';">
                                <div class="button-img-container">
                                    <img src="https://cdn-icons-png.flaticon.com/512/281/281760.png" class="button-img">
                                </div>
                                <h5 class="button-title">Warnings</h5>
                                <div class="button-description">A log of warnings issued to members</div>
                            </div>

                            <!-- Dashboard Button -->
                            <div class="content-box-button" onclick="location.href = '/logs/mutes';">
                                <div class="button-img-container">
                                    <img src="https://cdn-icons-png.flaticon.com/512/281/281760.png" class="button-img">
                                </div>
                                <h5 class="button-title">Mutes and Timeouts</h5>
                                <div class="button-description">A log of members who have been muted or timed out</div>
                            </div>

                            <!-- Dashboard Button -->
                            <div class="content-box-button" onclick="location.href = '/logs/bans';">
                                <div class="button-img-container">
                                    <img src="https://cdn-icons-png.flaticon.com/512/281/281760.png" class="button-img">
                                </div>
                                <h5 class="button-title">Bans and Unbans</h5>
                                <div class="button-description">A log of members who have been banned or unbanned</div>
                            </div>

                            <!-- Dashboard Button -->
                            <div class="content-box-button" onclick="location.href = '/logs/blacklists';">
                                <div class="button-img-container">
                                    <img src="https://cdn-icons-png.flaticon.com/512/281/281760.png" class="button-img">
                                </div>
                                <h5 class="button-title">Blacklisted Messages</h5>
                                <div class="button-description">A log of blacklisted messages</div>
                            </div>

                            <!-- Dashboard Button -->
                            <div class="content-box-button" onclick="location.href = '/logs/commands';">
                                <div class="button-img-container">
                                    <img src="https://cdn-icons-png.flaticon.com/512/281/281760.png" class="button-img">
                                </div>
                                <h5 class="button-title">Command Usage</h5>
                                <div class="button-description">A log of commands that have been used in the server
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--  -->
                </div>
            </div>
            <!--  -->



            <%- include('partials/footer') -%>